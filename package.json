{
  "name": "kswedberg",
  "displayName": "Dark and Light Theme",
  "description": "An attempt to make a VS Code theme with light editor but dark UI features",
  "publisher": "kswedberg",
  "version": "0.3.11",
  "engines": {
    "vscode": "^1.37.0"
  },
  "main": "./src/extension.js",
  "scripts": {
    "pub": "vsce package && vsce publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kswedberg/vscode-theme-kswedberg"
  },
  "categories": [
    "Themes"
  ],
  "icon": "screenshot.png",
  "keywords": [
    "theme",
    "light editor",
    "dark sidebar",
    "light",
    "dark"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.fix_cursor_color.enable",
        "title": "Cursor Color Fix: Enable"
      },
      {
        "command": "extension.fix_cursor_color.disable",
        "title": "Cursor Color Fix: Disable"
      }
    ],
    "themes": [
      {
        "label": "kswedberg",
        "uiTheme": "vs-dark",
        "path": "./themes/kswedberg-color-theme.json"
      }
    ]
  },
  "license": "MIT"
}
